<?xml version="1.0" encoding="UTF-8"?>
<ser:proxyServiceEntry xmlns:ser="http://www.bea.com/wli/sb/services" xmlns:oper="http://xmlns.oracle.com/servicebus/proxy/operations" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <ser:coreEntry isAutoPublish="false">
        <ser:description/>
        <ser:binding type="SOAP" isSoap12="false" xsi:type="con:SoapBindingType" xmlns:con="http://www.bea.com/wli/sb/services/bindings/config" xmlns:con1="http://www.bea.com/wli/sb/stages/config" xmlns:http="http://www.bea.com/wli/sb/transports/http" xmlns:env="http://www.bea.com/wli/config/env" xmlns:tran="http://www.bea.com/wli/sb/transports">
            <con:wsdl ref="CurrentAccount/LDAPAccountValidator/Resources/WSDLs/LDAPValidatorService"/>
            <con:port>
                <con:name>LDAPValidatorPort</con:name>
                <con:namespace>http://ws.validator.nextel.com.ar/</con:namespace>
            </con:port>
            <con:selector type="SOAP body"/>
        </ser:binding>
        <oper:operations enabled="true">
            <oper:monitoring enabled="true" aggregationInterval="10"/>
            <oper:sla-alerting enabled="true" level="normal"/>
        </oper:operations>
        <ser:ws-policy xmlns:con1="http://www.bea.com/wli/sb/stages/config" xmlns:con="http://www.bea.com/wli/sb/pipeline/config" xmlns:http="http://www.bea.com/wli/sb/transports/http" xmlns:env="http://www.bea.com/wli/config/env" xmlns:tran="http://www.bea.com/wli/sb/transports">
            <ser:binding-mode>wsdl-policy-attachments</ser:binding-mode>
        </ser:ws-policy>
        <ser:invoke ref="CurrentAccount/LDAPAccountValidator/Proxy Services/PSLDAPValidator" xsi:type="con:PipelineRef" xmlns:con="http://www.bea.com/wli/sb/pipeline/config"/>
    </ser:coreEntry>
    <ser:endpointConfig xmlns:con1="http://www.bea.com/wli/sb/stages/config" xmlns:con="http://www.bea.com/wli/sb/pipeline/config" xmlns:http="http://www.bea.com/wli/sb/transports/http" xmlns:env="http://www.bea.com/wli/config/env" xmlns:tran="http://www.bea.com/wli/sb/transports">
        <tran:provider-id>http</tran:provider-id>
        <tran:inbound>true</tran:inbound>
        <tran:URI>
            <env:value>/esb/CurrentAccount/LDAPAccountValidator/Proxy_Services/PSLDAPValidator</env:value>
        </tran:URI>
        <tran:inbound-properties/>
        <tran:all-headers>false</tran:all-headers>
        <tran:provider-specific>
            <http:inbound-properties/>
        </tran:provider-specific>
    </ser:endpointConfig>
</ser:proxyServiceEntry>