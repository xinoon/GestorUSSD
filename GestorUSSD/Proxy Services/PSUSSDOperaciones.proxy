<?xml version="1.0" encoding="UTF-8"?>
<ser:proxyServiceEntry xmlns:ser="http://www.bea.com/wli/sb/services" xmlns:oper="http://xmlns.oracle.com/servicebus/proxy/operations" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <ser:coreEntry isAutoPublish="false">
        <ser:binding type="SOAP" isSoap12="false" xsi:type="con:SoapBindingType" xmlns:con="http://www.bea.com/wli/sb/services/bindings/config" xmlns:con1="http://www.bea.com/wli/sb/stages/config" xmlns:http="http://www.bea.com/wli/sb/transports/http" xmlns:env="http://www.bea.com/wli/config/env" xmlns:tran="http://www.bea.com/wli/sb/transports" xmlns:con2="http://www.bea.com/wli/sb/stages/transform/config">
            <con:wsdl ref="GestorUSSD/Resources/WSDL/USSDOperaciones"/>
            <con:binding>
                <con:name>USSDOperacionesPortBinding</con:name>
                <con:namespace>http://ussd.operaciones.wom.ws.ncl.com</con:namespace>
            </con:binding>
            <con:selector type="SOAP body"/>
        </ser:binding>
        <oper:operations enabled="true">
            <oper:monitoring enabled="true" aggregationInterval="10"/>
            <oper:sla-alerting enabled="true" level="normal"/>
        </oper:operations>
        <ser:ws-policy xmlns:con1="http://www.bea.com/wli/sb/stages/config" xmlns:con="http://www.bea.com/wli/sb/pipeline/config" xmlns:http="http://www.bea.com/wli/sb/transports/http" xmlns:env="http://www.bea.com/wli/config/env" xmlns:tran="http://www.bea.com/wli/sb/transports" xmlns:con2="http://www.bea.com/wli/sb/stages/transform/config">
            <ser:binding-mode>wsdl-policy-attachments</ser:binding-mode>
        </ser:ws-policy>
        <ser:invoke ref="GestorUSSD/Proxy Services/PSUSSDOperaciones" xsi:type="con:PipelineRef" xmlns:con="http://www.bea.com/wli/sb/pipeline/config"/>
    </ser:coreEntry>
    <ser:endpointConfig xmlns:con1="http://www.bea.com/wli/sb/stages/config" xmlns:con="http://www.bea.com/wli/sb/pipeline/config" xmlns:http="http://www.bea.com/wli/sb/transports/http" xmlns:env="http://www.bea.com/wli/config/env" xmlns:tran="http://www.bea.com/wli/sb/transports" xmlns:con2="http://www.bea.com/wli/sb/stages/transform/config">
        <tran:provider-id>http</tran:provider-id>
        <tran:inbound>true</tran:inbound>
        <tran:URI>
            <env:value>/esb/GestorUSSD/Services/PSUSSDOperaciones</env:value>
        </tran:URI>
        <tran:inbound-properties/>
        <tran:all-headers>false</tran:all-headers>
        <tran:provider-specific xsi:type="http:HttpEndPointConfiguration">
            <http:inbound-properties/>
            <http:dispatch-policy>WorkManager-PSUSSDOperaciones</http:dispatch-policy>
        </tran:provider-specific>
    </ser:endpointConfig>
</ser:proxyServiceEntry>